doctype html
html(lang="ru" prefix="og: http://ogp.me/ns#")
	head
		meta(http-equiv="Content-Type" content="text/html; charset=UTF-8")
		meta(http-equiv="X-UA-Compatible" content="IE=EmulateIE9")
		meta(name="viewport" content="width=device-width, initial-scale=1")
		meta(name="SKYPE_TOOLBAR" content="SKYPE_TOOLBAR_PARSER_COMPATIBLE")
		meta(charset="utf-8")
		meta(name="yandex-verification" content="ad6b0f0f478e9f72")
		base(href="https://" + repo + "/")
		title= title
		link(rel="shortcut icon" type="image/x-icon" href="https://" + repo + "/favicon.ico")
		link(rel="icon" type="image/png" href="https://" + repo + "/favicon.png")
		meta(name="title" content=title)
		meta(name="longtitle" content=title)
		meta(name="keywords" content=keywords)
		meta(name="description" content=description)
		meta(name="site_name" content=title)
		meta(itemprop="image" content="https://" + repo + "/images/" + open_graph.image_16x9)
		meta(name="short_link" content="https://" + repo + "/")
		meta(property="og:title" content=title)
		meta(property="og:description" content=description)
		meta(property="og:url" content="https://" + repo + "/")
		meta(property="og:locale" content="ru_RU")
		meta(property="og:type" content="website")
		meta(property="og:site_name" content=title)
		meta(property="og:image" content="https://" + repo + "/images/" + open_graph.image_16x9)
		meta(property="og:image:width" content=open_graph.image_16x9_width)
		meta(property="og:image:height" content=open_graph.image_16x9_height)
		meta(property="og:image:type" content="image/jpeg")
		meta(property="og:image" content="https://" + repo + "/images/" + open_graph.image_1x1)
		meta(property="og:image:width" content=open_graph.image_1x1_width)
		meta(property="og:image:height" content=open_graph.image_1x1_width)
		meta(property="og:image:type" content="image/jpeg")
		link(type="text/css" rel="stylesheet" href="https://" + repo + "/css/main.css?hash=" + hash)
		//-
			script(type="text/javascript")
				|document.querySelectorAll("link").forEach(e=>{const t=new URL(e.href);if(e.rel=="stylesheet"&&new URL(t.href,document.baseURI).origin==location.origin){fetch(t.href).then(async function(t){const n=await t.text(),c=new Blob([n],{type:"text/css"}),o=URL.createObjectURL(c);e.setAttribute("href",o)})}});
	body
		header
			.wrapper
				.github-icons
					a(href="https://" + repo + "/")
						img(src="https://" + repo + "/images/" + logotype alt=nickname + " GitHub")
		main
			.wrapper
				h1= title
				.news
					.news-container
						each item, key in items
							include inc/item.pug
		footer
			.wrapper
				span.copyright
					|Copyright &copy; 
					= copyright
				span.developer
					|Developed by:  
					a(href="https://github.com/" + userName + "/" target="_blank")= nickname
		//- script(src="https://" + repo + "/js/plugins.js?hash=" + hash type="text/javascript")
		include inc/yandex.pug
		script(src="https://" + repo + "/js/main.js?hash=" + hash type="text/javascript")